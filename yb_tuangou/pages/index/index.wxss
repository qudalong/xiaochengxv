page {
  margin: 0;
  padding: 0;
  background-color: #eee;
}

.rule {
  position: fixed;
  top: 23%;
  right: 0;
  background: rgba(0, 0, 0, 0.4);
  color: #fff;
  border-radius: 30rpx 0 0 30rpx;
  padding: 10rpx 0;
  text-align: center;
  width: 210rpx;
}

.rule .img {
  width: 46rpx;
  height: 40rpx;
  margin-right: 6rpx;
}

.rule-wrap {
  background: #fff;
  border-radius: 20rpx;
  width: 80%;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 99999999;
}

.rule-wrap .title {
  background: #EF4423;
  font-size: 40rpx;
  text-align: center;
  padding: 30rpx 0;
  color: #fff;
  position: relative;
  border-radius: 20rpx 20rpx 0 0;
}

.close {
  position: absolute;
  height: 50rpx;
  background: rgba(0, 0, 0, 0.8);
  width: 50rpx;
  text-align: center;
  line-height: 50rpx;
  color: #fff;
  border-radius: 50%;
  top: -20rpx;
  right: -20rpx;
}

.rule-wrap .content {
  padding: 30rpx;
  line-height: 1.6;
  height: 400rpx;
  overflow-y: scroll;
  text-align: left;
}
.rule-wrap .content .item ,.fixTitle{
  padding: 10rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  text-align: center;
}
.rule-wrap .content .item view{
  flex: 1;
}
.fixTitle view{
 flex: 1;
}
.fixTitle {
 padding: 30rpx 30rpx 0 30rpx;
 font-weight: bold;
}
.rule-wrap .content .item:nth-child(even) {
  background: #FDEFD5;
}

